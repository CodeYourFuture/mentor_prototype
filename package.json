    "data": "hasura console",
    "release": "release-it"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^4.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.2",
    "prettier": "^2.5.1",
    "release-it": "^14.12.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,tsx,ts}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "-": "^0.0.1",
    "@faker-js/faker": "^6.0.0-beta.0",
