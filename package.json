{
  "name": "mentor-for-slack",
  "version": "0.3.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "^14.17.3"
  },
  "workspaces": [
    "packages/integrations",
    "packages/chatbot",
    "packages/api",
    "packages/reports"
  ],
  "scripts": {
    "build": "tsc",
    "postinstall": "npm run build",
    "start": "node build/index.js",
    "talk-to-bot": "PORT=5000 nodemon --exec ts-node src/services/talk-to-bot/index.ts",
    "integrations:run": "ts-node src/services/integrations/scripts/run.ts",
    "integrations:cron": "ts-node src/services/integrations/scripts/cron.ts",
    "export-to-sheets:run": "ts-node src/services/export-to-sheets/scripts/run.ts",
    "export-to-sheets:cron": "ts-node src/services/export-to-sheets/scripts/cron.ts",
    "reset": "hasura metadata apply && hasura seed apply",
    "dev": "docker-compose up",
    "data": "hasura console"
  },
  "dependencies": {
    "@slack/bolt": "^3.6.0",
    "@types/google-spreadsheet": "^3.2.1",
    "apollo-boost": "^0.4.9",
    "apollo-server-express": "^3.3.0",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "google-spreadsheet": "^3.2.0",
    "googleapis": "^89.0.0",
    "graphql": "^15.5.3",
    "javascript-stringify": "^2.1.0",
    "javascript-time-ago": "^2.3.8",
    "json-2-csv": "^3.14.4",
    "luxon": "^2.0.2",
    "node-cron": "^3.0.0",
    "node-fetch": "^3.0.0",
    "nodemon": "^2.0.4",
    "timeago.js": "^4.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.4.3",
    "uuid": "^8.3.2",
    "-": "^0.0.1",
    "@faker-js/faker": "^6.0.0-beta.0",
    "@types/uuid": "^8.3.4",
  },
  "devDependencies": {
    "@types/nodemon": "^1.19.0",
    "@release-it/conventional-changelog": "^4.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.2",
    "prettier": "^2.5.1",
    "release-it": "^14.12.3"
  }
}
