{
  "name": "mentor-for-slack",
  "workspaces": [
    "packages/integrations", 
    "packages/chatbot",
    "packages/api",
    "packages/reports"
  ],
  "description": "Mentor for slack",
  "license": "MIT",
  "author": "Dom Vinyard <i@dom.vin>",
  "scripts": {
    "reset": "hasura metadata apply && hasura seed apply",
